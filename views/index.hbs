<div class="row" style="padding:20px; margin-right:20px;">
  {{#each result}}
  <div class="col-sm-3" style="margin-top: 10px;">
    <div class="card bg-info col d-flex justify-content-cente" style="width: 18rem; padding:30px;">
      <img class="card-img-top" style="width:230px;height:230px;" src="/images/{{this.image}}">
      <div class="card-body">
        <h5 class="card-title">{{this.title}}</h5>
        <p class="card-text">Note:{{this.discrption}}.</p>
        <p class="card-text">RS:{{this.price}}.</p>
        <a href="#" class="btn btn-primary">Add-Cart</a>
      </div>
    </div>
  </div>
  {{/each}}
</div>

